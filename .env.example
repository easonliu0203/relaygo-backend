# Server Configuration
PORT=8080
NODE_ENV=production
CORS_ORIGIN=*

# Supabase Configuration
SUPABASE_URL=your-supabase-url
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_ANON_KEY=your-supabase-anon-key

# Firebase Configuration
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_CLIENT_EMAIL=your-firebase-client-email
FIREBASE_PRIVATE_KEY=your-firebase-private-key

# GoMyPay Configuration
GOMYPAY_MERCHANT_ID=your-merchant-id
GOMYPAY_HASH_KEY=your-hash-key
GOMYPAY_HASH_IV=your-hash-iv
GOMYPAY_TEST_MODE=true

# Resend Email Configuration
RESEND_API_KEY=your-resend-api-key
RESEND_FROM_EMAIL=send@relaygo.pro

# OpenAI Configuration (for translation)
OPENAI_API_KEY=your-openai-api-key

# Redis Configuration (optional)
REDIS_URL=your-redis-url

# Other Configuration
AUTO_DISPATCH_ENABLED=false

